{
  "name": "@alexbft0/fun-di",
  "private": false,
  "version": "0.3.1",
  "description": "Fun to use dependency injection framework",
  "keywords": [
    "DI",
    "IoC",
    "dependency injection",
    "typescript"
  ],
  "author": "alexbft (https://github.com/alexbft)",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexbft/fun-di.git"
  },
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "lint": "biome lint && tsc --noEmit",
    "build": "rolldown -c",
    "test": "vitest",
    "dev:pack": "pnpm build && pnpm pack --pack-destination=dist-pack",
    "dev:publish": "pnpm build && pnpm publish"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "rolldown": "1.0.0-beta.32",
    "rolldown-plugin-dts": "^0.15.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "@ungap/with-resolvers": "^0.1.0"
  }
}
